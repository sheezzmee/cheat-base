{
    "version": 2,
    "builds": [
        {
            "src": "*.mjs",
            "use": "@vercel/node"
        },
        {
            "src": "client/dist/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "/"
        }
    ]
}
